{
  "package": {
    "dependencies": {
      "@craco/craco": "^7.0.0",
      "node-sass": "^7.0.3",
      "react": "^18.2.0",
      "react-dom": "^18.2.0",
      "react-router-dom": "^6.5.0",
      "react-scripts": "5.0.1",
      "typescript": "^4.9.4",
      "web-vitals": "^2.1.4"
    },
    "scripts": {
      "start": "craco start",
      "build": "craco build",
      "test": "npm run test:unit && npm run test:e2e",
      "test:unit": "jest --passWithNoTests --config=jest.config.js",
      "test:unit:watch": "jest --watch --config=jest.config.js",
      "test:e2e": "npm run cy:run",
      "test:e2e:watch": "npm run cy:open",
      "cy:open": "cypress open",
      "cy:run": "cypress run",
      "lint": "eslint '{src,tests}/**/*.{js,jsx,ts,tsx,json}'",
      "lint:fix": "eslint --fix '{src,tests}/**/*.{js,jsx,ts,tsx,json}'",
      "format": "prettier --write '{src,tests}/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc"
    },
    "devDependencies": {
      "@faker-js/faker": "^7.6.0",
      "@swc/core": "^1.3.23",
      "@swc/jest": "^0.2.24",
      "@testing-library/cypress": "^9.0.0",
      "@testing-library/jest-dom": "^5.16.5",
      "@testing-library/react": "^13.4.0",
      "@testing-library/user-event": "^13.5.0",
      "@types/jest": "^27.5.2",
      "@types/jest-axe": "^3.5.5",
      "@types/node": "^16.18.10",
      "@types/react": "^18.0.26",
      "@types/react-dom": "^18.0.9",
      "@typescript-eslint/eslint-plugin": "^5.46.1",
      "@typescript-eslint/parser": "^5.46.1",
      "cypress": "^12.1.0",
      "eslint": "^8.30.0",
      "eslint-config-prettier": "^8.5.0",
      "eslint-import-resolver-typescript": "^3.5.2",
      "eslint-plugin-import": "^2.26.0",
      "eslint-plugin-prettier": "^4.2.1",
      "eslint-plugin-react": "^7.31.11",
      "eslint-plugin-react-hooks": "^4.6.0",
      "jest": "^27.5.1",
      "jest-axe": "^7.0.0",
      "jest-transform-stub": "^2.0.0",
      "prettier": "^2.8.1"
    },
    "eslintConfig": {
      "extends": [
        "react-app",
        "react-app/jest"
      ]
    },
    "browserslist": {
      "production": [
        ">0.2%",
        "not dead",
        "not op_mini all"
      ],
      "development": [
        "last 1 chrome version",
        "last 1 firefox version",
        "last 1 safari version"
      ]
    }
  }
}